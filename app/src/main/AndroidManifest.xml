<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.luyinbros.demo">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


    <application
        android:name=".Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.BundleValueActivity" />
        <activity android:name=".activity.OnPermissionResultActivity" />
        <activity android:name=".activity.BindViewActivity" />
        <activity android:name=".activity.LifecycleEventActivity" />
        <activity android:name=".activity.FragmentAndOtherActivity" />
        <activity android:name=".activity.OnActivityResultActivity" />
        <activity android:name=".activity.OnActivityResultActivity$StubActivity" />


        <activity android:name=".tree.RootActivity"/>
        <activity android:name=".tree.AActivity"/>
        <activity android:name=".tree.BActivity"/>
        <activity android:name=".tree.CActivity"/>
        <activity android:name=".tree.DActivity"/>
        <activity android:name=".tree.EActivity"/>
        <activity android:name=".tree.FActivity"/>

    </application>

</manifest>